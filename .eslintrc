{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-else-return": 0,
    "no-use-before-define": 0,
    "no-useless-return": 0,
    "no-alert": 0,
    "array-callback-return": 0,
    "import/no-mutable-exports": 0
  }
}